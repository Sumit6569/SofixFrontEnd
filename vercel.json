{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_ENV": "production",
    "VITE_PRODUCTION_API_BASE": "https://sofixbackend.onrender.com",
    "VITE_PRODUCTION_VERIFY_ORIGIN": "https://sofixs.com"
  }
}
